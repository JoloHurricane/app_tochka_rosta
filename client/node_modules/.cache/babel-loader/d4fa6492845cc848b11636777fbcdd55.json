SC,KAAT,QAAsB,cAAtB;AACA,SAAQC,MAAR,QAAqB,sBAArB;;;;AAEA,SAASC,SAAT,GAAoB;AAAA;;AAChBJ,EAAAA,SAAS,CAAC,MAAI;AACV,QAAIK,KAAK,GAAGC,QAAQ,CAACC,gBAAT,CAA0B,WAA1B,CAAZ;AACA,QAAIC,SAAS,GAAGP,CAAC,CAACQ,QAAF,CAAWC,IAAX,CAAgBL,KAAhB,CAAhB;AACH,GAHQ,CAAT;AAKA,QAAM,CAACM,KAAD,EAAOC,QAAP,IAAmBC,QAAQ,CAAC;AAC9BC,IAAAA,IAAI,EAAC;AADyB,GAAD,CAAjC;AAIE,QAAMC,QAAQ,GAAGC,WAAW,CAAC,YAAY;AACrC,QAAI;AACA,YAAMC,SAAS,GAAG,MAAMC,OAAO,CAAC,mBAAD,EAC/B,MAD+B,EACvB,EADuB,EACpB,EADoB,EACjB,kBADiB,CAA/B;AAGAN,MAAAA,QAAQ,CAAC,EAAC,GAAGO;AAAJ,OAAD,CAAR;AACAC,MAAAA,OAAO,CAACC,GAAR,CAAYF,OAAZ;AAEP,KAPG,CAOF,OAAOG,CAAP,EAAU,CAAE;AACf,GAT6B,EAS3B,CAACJ,OAAD,CAT2B,CAA5B;AAWFlB,EAAAA,SAAS,CAAC,MAAM;AACde,IAAAA,QAAQ;AACT,GAFQ,EAEP,CAACA,QAAD,CAFO,CAAT;;AAIE,MAAGQ,OAAH,EAAW;AACT,wBAAO,QAAC,MAAD;AAAA;AAAA;AAAA;AAAA,YAAP;AACH;;AACD,sBACI;AAAA,4BACC;AAAK,MAAA,KAAK,EAAE;AAACC,QAAAA,eAAe,EAAC,MAAjB;AAAwBC,QAAAA,QAAQ,EAAC;AAAjC,OAAZ;AAAA,8BAEN;AAAK,QAAA,SAAS,EAAC,oBAAf;AAAA,+BACM;AAAK,UAAA,SAAS,EAAC,UAAf;AAAA,iCAA0B;AAAK,YAAA,SAAS,EAAC,EAAf;AAAkB,YAAA,GAAG,EAAC,KAAtB;AAA4B,YAAA,GAAG,EAAC;AAAhC;AAAA;AAAA;AAAA;AAAA;AAA1B;AAAA;AAAA;AAAA;AAAA;AADN;AAAA;AAAA;AAAA;AAAA,cAFM,eAMJ;AAAM,QAAA,SAAS,EAAC,8CAAhB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,cANI;AAAA;AAAA;AAAA;AAAA;AAAA,YADD,eASJ;AAAK,MAAA,SAAS,EAAC,eAAf;AAAA,6BACI;AAAG,QAAA,SAAS,EAAC,WAAb;AAAA,okGAKR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBALQ,eAQR;AAAI,UAAA,SAAS,EAAC,iBAAd;AAAA,kCACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBADJ,eAKI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBALJ;AAAA;AAAA;AAAA;AAAA;AAAA,gBARQ,eAkBR;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gBAlBQ,eAqBR;AAAI,UAAA,SAAS,EAAC,iBAAd;AAAA,kCACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBADJ,eAMI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBANJ;AAAA;AAAA;AAAA;AAAA;AAAA,gBArBQ;AAAA;AAAA;AAAA;AAAA;AAAA;AADJ;AAAA;AAAA;AAAA;AAAA,YATI;AAAA,kBADJ;AAiDH;;GA7EQrB,S;;KAAAA,S;AA8ET,eAAeA,SAAf","sourcesContent":["import React,{useEffect} from 'react'\r\nimport M from 'materialize-css'\r\nimport { Route } from 'react-router';\r\nimport {Loader} from \"../components/Loader\"\r\n\r\nfunction AboutPage(){\r\n    useEffect(()=>{\r\n        var elems = document.querySelectorAll('.parallax');\r\n        var instances = M.Parallax.init(elems, );\r\n    },)\r\n\r\n    const [about,setAbout] = useState({\r\n        text:'',\r\n      })\r\n    \r\n      const fetchOrg = useCallback(async () => {\r\n          try {\r\n              const fetouthed = await request('/api/abi-edit/abi', \r\n              'POST', {},{},'application/json')\r\n              \r\n              setAbout({...fetched})\r\n              console.log(fetched)\r\n             \r\n      } catch (e) {}\r\n    }, [request])\r\n    \r\n    useEffect(() => {\r\n      fetchOrg()\r\n    },[fetchOrg])\r\n    \r\n      if(loading){\r\n        return <Loader/>\r\n    }\r\n    return(\r\n        <>\r\n         <div style={{backgroundColor:\"#000\",position:\"relative\"}}>\r\n   \r\n   <div className=\"parallax-container\">\r\n         <div className=\"parallax\"><img className=\"\" alt=\"asd\" src=\"/images/about3.jpg\"></img></div>\r\n       \r\n     </div>\r\n     <h2   className=\"title-parallax color white-text center-align\">О проекте</h2> \r\n    </div>\r\n    <div className=\"row container\">\r\n        <p className=\"flow-text\"> Центры «Точка роста» — специальные образовательные центры, создаваемые на базе школ в селах и малых городах. Их работа направлена на подготовку детей по цифровому, естественно-научному, техническому и гуманитарному профилям. Их открытие предусмотрено федеральным проектом «Современная школа», входящим в национальный проект «Образование».\r\n\r\nСовокупность образовательных организаций, на базе которых создаются Центры образования цифрового и гуманитарного профилей «Точка роста», составит федеральную сеть Центров.\r\n\r\nК 2024 году общее число Центров на территории Калининградской области достигнет 71. \r\n<h4>\r\nЦели деятельности центров:\r\n</h4>\r\n<ul className=\"browser-default\">\r\n    <li>создание условий для внедрения на уровнях начального общего, \r\n        основного общего и среднего общего образования новых методов обучения и воспитания, \r\n        образовательных технологий, обеспечивающих освоение обучающимися основных и дополнительных \r\n        общеобразовательных программ цифрового, естественнонаучного, технического и гуманитарного профилей; </li>\r\n    <li>\r\n    обновление содержания и совершенствование методов обучения предметных областей «Технология», \r\n    «Математика и информатика», «Физическая культура и основы безопасности жизнедеятельности». \r\n    </li>\r\n</ul>\r\n<h4>\r\nЗадачи центров:\r\n</h4>\r\n<ul className=\"browser-default\">\r\n    <li>\r\n    100% охват контингента обучающихся образовательной организации, \r\n    осваивающих основную общеобразовательную программу по предметным областям «Технология», «Математика и информатика», «Физическая культура и основы \r\n    безопасности жизнедеятельности» на обновленном оборудовании с применением новых методик обучения и воспитания;\r\n    </li>\r\n    <li>\r\n    не менее 70% охват контингента обучающихся дополнительными общеобразовательными программами цифрового, \r\n    естественнонаучного, технического и гуманитарного профилей во внеурочное время, \r\n    в том числе с использованием дистанционных форм обучения и сетевого партнерства. \r\n    </li>\r\n</ul>\r\n</p>\r\n    </div>\r\n        </>\r\n    )\r\n\r\n}\r\nexport default AboutPage"]},"metadata":{},"sourceType":"module"}